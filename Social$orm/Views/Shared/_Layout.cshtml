<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Social Form</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />

    @*added*@
    <link rel="stylesheet" href="//cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css" />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-toggleable-lg navbar-dark bg-info border-bottom box-shadow mb-3 ">
        @*<nav class="navbar navbar-expand-lg navbar-dark bg-info">*@
        <div class="container">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Social Form</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                @*<partial name="_LoginPartial" />*@
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" style="margin-left:30px;margin-right:30px;" asp-area="" asp-controller="Beneficiars" asp-action="Index">Beneficiaries</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" style="margin-left:30px;" asp-area="" asp-controller="Aids" asp-action="Index">See all sent aids </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" style="margin-left:30px;" asp-area="" asp-controller="Aids" asp-action="Create">Giving Help </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" style="margin-left:30px;" asp-area="" asp-controller="Home" asp-action="ChooseBeneficiary">Edit a Beneficiary's info</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link text-dark" style="margin-left:30px;" asp-area="" asp-controller="Report" asp-action="FamilyReports">All Reports</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" style="margin-left:30px;" asp-area="" asp-controller="Report" asp-action="SpecialFormForAFamily">Family Report</a>
                    </li>
                </ul>
            </div>
        </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - Social_Form - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @*added*@
    <script src="//cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
