
@{
    ViewData["Title"] = "Find";
}

<h1>Select the Beneficiary's Fullname :</h1>

<hr />
<br>
<div class="row">
    <div class="col-md-4">
        <form asp-action="ChooseField">
            <div class="form-group">
                <select name="BeneficiarID" id="BeneficiarID" class="form-control" asp-items="ViewBag.BeneficiarID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Okay" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>

